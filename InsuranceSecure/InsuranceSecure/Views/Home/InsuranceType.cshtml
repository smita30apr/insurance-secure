@model Insurance

@{ 
    switch (Model.GetType().Name)
    {
        case "Life":
            @Html.Partial("Insurance/Life", (Life)Model)
            break;
        case "Vehicle":
            @Html.Partial("Insurance/Vehicle", (Vehicle)Model)
            break;
        case "Health":
            @Html.Partial("Insurance/Health", (Health)Model)
            break;
        default:
            break;
    }
}
